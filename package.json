{
  "name": "logux-vuex",
  "version": "0.1.2",
  "description": "Vuex compatible API for Logux",
  "keywords": [
    "logux",
    "client",
    "vuex"
  ],
  "author": "Nikolay Govorov <nikolay_govorov@bk.ru>",
  "repository": "nikolay-govorov/logux-vuex",
  "dependencies": {
    "logux-client": "^0.2.6",
    "logux-core": "^0.2.1",
    "logux-sync": "^0.2.3",
    "nanoclone": "^0.1.2",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "docdash": "^0.4.0",
    "eslint": "^4.14.0",
    "eslint-config-logux": "^17.0.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-es5": "^1.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.5.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "jest": "^21.2.1",
    "jsdoc": "^3.5.5",
    "lint-staged": "^6.0.0",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.6.2",
    "size-limit": "^0.13.2",
    "vue": "^2.5.13",
    "yaspeller-ci": "^1.0.0"
  },
  "scripts": {
    "lint-staged": "lint-staged",
    "spellcheck": "yarn docs && yaspeller-ci *.md docs/*.html",
    "clean": "rimraf coverage/ docs/",
    "lint": "eslint *.js test/{**/,}*.js",
    "docs": "jsdoc --configure .jsdocrc *.js",
    "test": "jest --coverage && npm run lint && size-limit && yarn spellcheck"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "statements": 100
      }
    }
  },
  "eslintConfig": {
    "extends": "eslint-config-logux/browser"
  },
  "size-limit": [
    {
      "path": "index.js",
      "limit": "13.3 KB"
    }
  ],
  "lint-staged": {
    "*.md": "yaspeller-ci",
    "*.js": "eslint"
  },
  "pre-commit": [
    "lint-staged"
  ]
}
